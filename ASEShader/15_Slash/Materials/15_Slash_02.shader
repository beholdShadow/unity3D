				float temp_output_20_0 = ( pow( ( smoothstepResult32 - smoothstepResult34 ) , ( 10.0 - _BladeSpread ) ) * pow( ( 1.0 - texCoord15.y ) , ( 10.0 - _Strength ) ) );
